[derle]
betik   = make ${makejobs} COMPONENT_TYPE=lib-shared PREFIX=/usr
betik   = make ${makejobs} COMPONENT_TYPE=lib-static PREFIX=/usr

[pakur]
betik   = make COMPONENT_TYPE=lib-shared PREFIX=/usr DESTDIR=${PKG} install
betik   = make COMPONENT_TYPE=lib-static PREFIX=/usr DESTDIR=${PKG} install
